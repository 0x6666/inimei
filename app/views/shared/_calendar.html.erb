<div id="CalendarMain">
  <div id="title">
    <a class="selectBtn prevMonth" href="javascript:" onclick="CalendarHandler.CalendarLast();"><</a>
    <a class="selectBtn selectYear" href="javascript:" onClick="CalendarHandler.CreateSelectYear(CalendarHandler.showYearStart);">2014年</a>
    <a class="selectBtn selectMonth" onClick="CalendarHandler.CreateSelectMonth()">6月</a>
    <a class="selectBtn nextMonth" href="javascript:" onClick="CalendarHandler.CalendarNext();">></a>
    <a class="selectBtn currentDay" href="javascript:" onClick="CalendarHandler.CreateCurrentCalendar(0,0,0);">今天</a>
  </div>
  <div id="context">
    <div id="center">
      <div id="centerMain">
        <div id="selectYearDiv"></div>
        <div id="centerCalendarMain">
          <div class="week" id="week">
            <h3> 一 </h3>

            <h3> 二 </h3>

            <h3> 三 </h3>

            <h3> 四 </h3>

            <h3> 五 </h3>

            <h3> 六 </h3>

            <h3> 日 </h3>
          </div>
          <div id="Container"></div>
        </div>
        <div id="selectMonthDiv"></div>
      </div>
    </div>
    <!--div id="foots"><span id="footNow">19:14:34</span></div-->
  </div>
</div>

<script type="text/javascript">
    $(document).ready(function (e) {
        <% if @select_date %>
        CalendarHandler.initialize(<%= @select_date.year %>, <%= @select_date.month %>, <%= @select_date.day %>, '<%= schedules_user_path(current_user)%>');
        <% else %>
        CalendarHandler.initialize(0, 0, 0, '');
        <% end %>
    });
</script>